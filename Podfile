project 'paths.xcodeproj'
platform :ios, '10.0'
use_frameworks!

target 'paths' do
    # Pods for paths
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'
    pod 'RxDataSources', '~> 3.0'
    pod 'RxCoreData', '~> 0.4.0'
    pod 'Eureka', '~> 4.0'
    pod 'SwiftyBeaver'
    pod 'SwiftSimplify'
    pod 'AssetsPickerViewController', '~> 2.0'

  target 'pathsTests' do
   inherit! :complete
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
    pod 'Quick'
    pod 'Nimble'
    pod 'RandomKit', '~> 5.2.3'
  end

  target 'pathsUITests' do
  inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

end
