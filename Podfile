project 'paths.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
use_frameworks!

target 'paths' do
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'
    pod 'RxDataSources', '~> 3.0'
    pod 'RxCoreData', '~> 0.4.0'
    pod 'Eureka', '~> 4.0'
    pod 'SwiftyBeaver'
    pod 'SwiftSimplify'
    # Pods for paths

  target 'pathsTests' do
   inherit! :complete
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
    pod 'Quick'
    pod 'Nimble'
  end

  target 'pathsUITests' do
  inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

end
